{
  "name": "cyklokoalicia/opensource-bikeshare",
  "description": "The world's first low-cost and open source bike sharing system.",
  "minimum-stability": "stable",
  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "Volodymyr Panivko",
      "email": "sveneld300@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "BikeShare\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BikeShare\\Test\\": "tests/"
    }
  },
  "require": {
    "php": "^8.1",
    "phpmailer/phpmailer": "^6.5.0",
    "tecnickcom/tcpdf": "^6.2.6",
    "ext-json": "*",
    "ext-mysqli": "*",
    "ext-gettext": "*",
    "ext-curl": "*",
    "ext-pdo": "*",
    "monolog/monolog": "^1.27",
    "symfony/dependency-injection": "^6.0",
    "symfony/http-kernel": "^6.0",
    "symfony/framework-bundle": "^6.0",
    "symfony/http-foundation": "^6.0",
    "symfony/config": "^6.0",
    "symfony/expression-language": "^6.0",
    "symfony/monolog-bundle": "^3.10",
    "symfony/console": "^6.0",
    "symfony/runtime": "^6.0",
    "symfony/twig-bundle": "^6.0",
    "symfony/dotenv": "^6.0",
    "symfony/translation": "^6.0",
    "symfony/asset": "^6.0",
    "symfony/security-bundle": "^6.0",
    "symfony/event-dispatcher": "^6.0",
    "symfony/form": "^6.0",
    "sentry/sentry-symfony": "^5.1",
    "symfony/clock": "^6.4",
    "giggsey/libphonenumber-for-php": "^8.13"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.10|^4.0",
    "phpunit/phpunit": "^9.6",
    "php-mock/php-mock-phpunit": "^2.13",
    "rector/rector": "^2.1",
    "symfony/browser-kit": "^6.0",
    "symfony/test-pack": "^1.1",
    "symfony/stopwatch": "^6.0",
    "nelmio/alice": "^3.9",
    "slevomat/coding-standard": "^8.19"
  },
  "config": {
    "allow-plugins": {
      "symfony/runtime": true,
      "symfony/flex": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "scripts": {
    "test": [
      "Composer\\Config::disableProcessTimeout",
      "@putenv APP_ENV=test",
      "php bin/console cache:clear",
      "php bin/console load:fixtures",
      "phpunit --configuration phpunit.xml.dist"
    ]
  }
}
