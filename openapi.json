{
  "openapi": "3.0.2",
  "info": {
    "title": "Open Source Bike Share",
    "version": "0.0.1"
  },
  "servers": [
    {
      "url": "https://api.server.test/v1"
    }
  ],
  "paths": {
    "/stand": {
      "get": {
        "description": "Get all stands",
        "tags": [
          "Stand"
        ],
        "operationId": "stand.get",
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "*/*": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/Stand"
                  }
                }
              }
            }
          },
          "default": {
            "description": "error payload",
            "content": {
              "text/html": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            }
          }
        },
        "security": [
        ]
      }
    },
    "/user/register": {
      "post": {
        "description": "New user registration",
        "tags": [
          "User"
        ],
        "operationId": "user.register",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/User"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful registration",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/User"
                }
              }
            }
          },
          "default": {
            "description": "error payload",
            "content": {
              "text/html": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorModel"
                }
              }
            }
          }
        },
        "security": [
        ]
      }
    }
  },
  "components": {
    "securitySchemes": {
      "api_key": {
        "type": "apiKey",
        "name": "api_key",
        "in": "header"
      }
    },
    "schemas": {
      "Stand": {
        "type": "object",
        "required": [
          "name",
          "address",
          "latitude",
          "longitude"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "Stand name"
          },
          "address": {
            "type": "string",
            "description": "Stand address"
          },
          "latitude": {
            "type": "number",
            "description": "Stand latitude"
          },
          "longitude": {
            "type": "number",
            "description": "Stand longitude"
          }
        }
      },
      "User": {
        "type": "object",
        "required": [
          "fullname",
          "email",
          "phone",
          "password"
        ],
        "properties": {
          "fullname": {
            "type": "string",
            "description": "Full name"
          },
          "email": {
            "type": "string",
            "description": "Email address"
          },
          "phone": {
            "type": "string",
            "description": "Phone number"
          },
          "password": {
            "type": "password",
            "description": "Password"
          }
        }
      },
      "ErrorModel": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer"
          },
          "message": {
            "type": "string"
          }
        }
      }
    }
  }
}
